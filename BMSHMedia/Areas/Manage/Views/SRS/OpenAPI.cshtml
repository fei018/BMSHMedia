@model BMSHMedia.ViewModel.SRSVMs.SRSOpenAPIVM

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.One">
        <wt:textbox field="Entity.RTMPUrl" empty-text="rtmp://live.bmsh.info/live/bmsh"/>
        <wt:textbox field="Entity.PlayUrl" empty-text="http://live.bmsh.info/live/bmsh.flv"/>
        <wt:textbox field="Entity.ApiUrl_PublishKey" label-text="PublishSecret ApiUrl" empty-text="查詢直播發佈的secret api url" />
        <wt:textbox field="Entity.ApiSecret" />
    </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>